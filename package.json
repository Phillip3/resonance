  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./components"
      ],
      "pattern": "**/*.stories.js"
    }
  }
}
