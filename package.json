{
  "scripts": {
    "start": "react-native start",
    "start:reset-cache": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "tunnel": "expo start --tunnel",
    "publish": "npx expo-optimize . && expo publish --release-channel=develop_1_0_3",
    "test": "node ./node_modules/jest-expo/bin/jest.js --watchAll=false",
    "test:watch": "node ./node_modules/jest-expo/bin/jest.js --watchAll",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.js\"",
    "react-devtools": "react-devtools",
    "clear": ". scripts/clear.sh",
    "nuke": "rm -rf node_modules/ && rm -f package-lock.json && npm cache verify && npm install && expo r -c",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.2.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.7.1",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.7.0",
    "amplitude-js": "^7.1.0",
    "axios": "^0.19.0",
    "expo": "^37.0.11",
    "expo-asset": "~8.1.5",
    "expo-blur": "~8.1.2",
    "expo-font": "~8.2.1",
    "expo-splash-screen": "^0.3.1",
    "expo-updates": "^0.2.13",
    "expo-web-browser": "~8.3.1",
    "fast-html-parser": "^1.0.1",
    "formik": "^2.1.4",
    "html-entities": "^1.2.1",
    "react": "~16.9.0",
    "react-content-loader": "^5.1.0",
    "react-dom": "~16.13.0",
    "react-native": "~0.61.5",
    "react-native-device-info": "^5.6.2",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-hyperlink": "0.0.19",
    "react-native-modal": "^11.5.6",
    "react-native-onesignal": "^3.9.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-redash": "^14.2.2",
    "react-native-safe-area-context": "0.7.3",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "~2.8.0",
    "react-native-svg": "12.1.0",
    "react-native-unimodules": "^0.10.1",
    "react-native-web": "~0.13.3",
    "react-native-webview": "^10.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.19",
    "@storybook/react-native": "^5.3.18",
    "@storybook/react-native-server": "^5.3.19",
    "babel-eslint": "^10.0.1",
    "babel-jest": "~26.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.2.3",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.22.1",
    "husky": "^4.2.3",
    "jest": "~26.1.0",
    "jest-expo": "^37.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-devtools": "^4.8.1",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-testing-library": "^1.14.0",
    "react-test-renderer": "^16.12.0"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./components"
      ],
      "pattern": "**/*.stories.js"
    }
  }
}
