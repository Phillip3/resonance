{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "publish": "expo optimize && expo publish",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll=false",
    "test:watch": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "eslint .",
    "flow": "flow",
    "format": "prettier --write \"**/*.js\"",
    "prepush": "npm run flow && npm test",
    "react-devtools": "react-devtools"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^9.0.0",
    "axios": "^0.19.0",
    "expo": "^33.0.0",
    "expo-asset": "^5.0.1",
    "expo-blur": "^5.0.1",
    "expo-font": "^5.0.1",
    "expo-web-browser": "~5.0.2",
    "react": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-dropdownalert": "^4.1.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-modal": "^11.1.0",
    "react-native-svg": "^9.5.1",
    "react-navigation": "^3.11.0",
    "rn-placeholder": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^2.21.2",
    "flow-bin": "^0.98.1",
    "husky": "^3.0.0",
    "jest-expo": "^33.0.2",
    "lint-staged": "^9.1.0",
    "prettier": "^1.17.1",
    "react-devtools": "^3.6.1"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
