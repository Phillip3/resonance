{
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "tunnel": "expo start --tunnel",
    "publish": "npx expo-optimize . && expo publish --release-channel=develop_1_0_2",
    "test": "node ./node_modules/jest-expo/bin/jest.js --watchAll=false",
    "test:watch": "node ./node_modules/jest-expo/bin/jest.js --watchAll",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.js\"",
    "react-devtools": "react-devtools",
    "clear": ". scripts/clear.sh",
    "nuke": "rm -rf node_modules/ && rm -f package-lock.json && npm cache verify && npm install && expo r -c",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/bottom-tabs": "^5.2.6",
    "@react-navigation/native": "^5.1.4",
    "@react-navigation/stack": "^5.2.10",
    "axios": "^0.19.0",
    "expo": "^37.0.0",
    "expo-analytics-amplitude": "~8.1.0",
    "expo-asset": "~8.1.4",
    "expo-blur": "~8.1.0",
    "expo-font": "~8.1.0",
    "expo-splash-screen": "^0.2.0",
    "expo-updates": "~0.1.0",
    "expo-web-browser": "~8.1.0",
    "fast-html-parser": "^1.0.1",
    "html-entities": "^1.2.1",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "~0.61.5",
    "react-native-dropdownalert": "^4.1.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-hyperlink": "0.0.19",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "~1.7.0",
    "react-native-redash": "^10.0.1",
    "react-native-safe-area-context": "0.7.3",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "~2.2.0",
    "react-native-svg": "11.0.1",
    "react-native-unimodules": "~0.9.0",
    "react-native-web": "~0.11.7",
    "react-native-webview": "8.1.1",
    "rn-placeholder": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react-native": "^5.3.18",
    "@storybook/react-native-server": "^5.3.18",
    "babel-eslint": "^10.0.1",
    "babel-jest": "~25.2.6",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.17.24",
    "husky": "^4.2.3",
    "jest": "~25.2.6",
    "jest-expo": "^37.0.0",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "react-devtools": "^4.6.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-testing-library": "^1.14.0",
    "react-test-renderer": "^16.12.0"
