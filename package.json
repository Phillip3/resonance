{
  "scripts": {
    "start": "react-native start",
    "start:reset-cache": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "tunnel": "expo start --tunnel",
    "publish": "npx expo-optimize . && expo publish --release-channel=develop_1_0_4",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll=false",
    "test:watch": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.js\"",
    "react-devtools": "react-devtools",
    "clear": ". scripts/clear.sh",
    "nuke": "rm -rf node_modules/ && rm -f package-lock.json && npm cache verify && npm install && expo r -c",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/async-storage": "~1.12.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.7.1",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.7.0",
    "amplitude-js": "^7.1.0",
    "axios": "^0.20.0",
    "expo": "^39.0.0",
    "expo-asset": "~8.2.0",
    "expo-blur": "~8.2.0",
    "expo-font": "~8.3.0",
    "expo-splash-screen": "~0.6.1",
    "expo-updates": "~0.3.3",
    "expo-web-browser": "~8.5.0",
    "fast-html-parser": "^1.0.1",
    "formik": "^2.1.5",
    "html-entities": "^1.2.1",
    "react": "16.13.1",
    "react-content-loader": "^5.1.0",
    "react-dom": "16.13.1",
    "react-native": "0.63.3",
    "react-native-device-info": "^5.6.2",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-hyperlink": "0.0.19",
    "react-native-modal": "^11.5.6",
    "react-native-onesignal": "^3.9.0",
    "react-native-reanimated": "~1.13.0",
    "react-native-redash": "^14.2.4",
    "react-native-safe-area-context": "3.1.4",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "~2.10.1",
    "react-native-svg": "12.1.0",
    "react-native-unimodules": "~0.11.0",
    "react-native-web": "~0.13.7",
    "react-native-webview": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^6.0.26",
    "@storybook/react-native": "^5.3.18",
    "@storybook/react-native-server": "^5.3.19",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.22.1",
    "husky": "^4.2.3",
    "jest": "^26.4.2",
    "jest-expo": "^39.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-devtools": "^4.8.2",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-testing-library": "^1.14.0",
    "react-test-renderer": "^16.12.0"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./components"
      ],
      "pattern": "**/*.stories.js"
    }
  }
}
